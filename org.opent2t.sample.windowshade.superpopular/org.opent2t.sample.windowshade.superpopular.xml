<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://standards.freedesktop.org/dbus/introspect-1.0.dtd"[]>
<node>
  <interface name="org.opent2t.sample.windowshade.superpopular">
    <!-- open/raise the window treatment -->
    <method name="open" />
    <!-- close/lower the window treatment -->
    <method name="close" />
    <!-- Called when there is an error during usage of this translator -->
    <signal name="error">
        <arg name="type" type="s" direction="out"/>
        <arg name="message" type="s" direction="out"/>
    </signal>
  </interface>
</node>