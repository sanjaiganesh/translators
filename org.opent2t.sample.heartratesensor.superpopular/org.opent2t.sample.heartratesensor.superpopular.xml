<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://standards.freedesktop.org/dbus/introspect-1.0.dtd"[]>
<node>
  <interface name="org.opent2t.sample.heartratesensor.superpopular">
    <!-- Get the current beats per minute -->
    <method name="getBeatsPerMinute">
      <arg name="beatsPerMinute" type="d" direction="out" />
    </method>
    <!-- Get the current rr-interval -->
    <method name="getRRInterval">
      <arg name="rrInterval" type="d" direction="out" />
    </method>
    <!-- Get the current energyExpended -->
    <method name="getEnergyExpended">
      <arg name="energyExpended" type="d" direction="out" />
    </method>
    <!-- Get the sensor contact state-->
    <method name="getContactStatus">
      <arg name="contactStatus" type="d" direction="out" />
    </method>
  </interface>
</node>